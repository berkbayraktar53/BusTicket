<body>
    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-image="~/Templates/Admin/assets/img/sidebar-5.jpg">
            <div class="sidebar-wrapper">
                <div class="logo" style="text-align:center">
                    ADMİN PANELİ
                </div>
                <ul class="nav">
                    <li class="active">
                        <a href="/Admin/Index">
                            <i class="pe-7s-home"></i>
                            <p>Anasayfa</p>
                        </a>
                    </li>
                    <li class="active">
                        <a href="/Admin/Customer">
                            <i class="pe-7s-users"></i>
